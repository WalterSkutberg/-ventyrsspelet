class Player:
    def __init__(self,name,hp,strg,lvl,inv):
        self.name = name
        self.hp = hp
        self.strg = strg
        self.lvl = lvl
        self.inv = inv


class Weapon:
    def __init__(self,name,dmg,bst):
        self.name = name
        self.dmg = dmg
        self.bst = bst

class Item:
    def __init__(self,name,attribute):
        self.name = name
        self.attribute=attribute
